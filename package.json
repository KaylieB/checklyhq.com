{
  "name": "checklyhq.com",
  "version": "1.0.0",
  "description": "Checkly public website",
  "repository": "checkly/checklyhq.com",
  "main": "index.js",
  "scripts": {
    "hugo": "gulp hugo",
    "build": "gulp build",
    "build-preview": "gulp build-preview",
    "start": "gulp server",
    "lint": "eslint src",
    "generate-api-docs": "curl -OL http://localhost:3000/swagger.json && widdershins --user_templates api-docs/widdershins-user-templates --resolve --search true --language_tabs 'shell:curl' --summary --maxDepth 0 swagger.json -o api-docs/shins/source/index.html.md && cd api-docs/shins && node shins --logo ../logo.png  --customcss",
    "deploy:cacheable": "s3-deploy './dist/**' --cwd './dist/' --region 'eu-central-1' --bucket 'checklyhq.com' --etag --cache 315360000",
    "deploy:non-cacheable": "s3-deploy './dist/**/*.html' --cwd './dist/' --region 'eu-central-1' --bucket 'checklyhq.com' --etag --cache 0",
    "deploy": "npm run deploy:non-cacheable && npm run deploy:cacheable",
    "deploy:production": "NODE_ENV=production npm generate-api-docs && npm run build && npm run deploy"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bootstrap": "4.0.0-alpha.6",
    "browser-sync": "^2.26.3",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.16.0",
    "exports-loader": "^0.7.0",
    "file-loader": "^1.1.11",
    "gulp": "^4.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-flatten": "^0.4.0",
    "gulp-inline-css": "3.3.1",
    "gulp-postcss": "^7.0.1",
    "gulp-pug": "^4.0.1",
    "gulp-rev-all": "^0.9.8",
    "gulp-sass": "^4.0.2",
    "gulp-watch": "^5.0.1",
    "html-loader": "^0.5.5",
    "hugo-bin": "0.39.0",
    "imports-loader": "^0.7.1",
    "postcss-cssnext": "3.1.0",
    "postcss-import": "^11.1.0",
    "pug-html-loader": "^1.1.5",
    "run-sequence": "^2.2.1",
    "sass-loader": "^6.0.7",
    "url-loader": "^0.6.2",
    "uuid": "^3.3.2",
    "webpack": "3.10.0",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "s3-deploy": "^1.2.1",
    "shins": "^2.3.2-4",
    "widdershins": "^3.6.7"
  }
}
